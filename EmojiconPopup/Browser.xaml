<Window x:Class="EmojiconPopup.Browser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tb="http://www.hardcodet.net/taskbar"
        xmlns:local="clr-namespace:EmojiconPopup"
        mc:Ignorable="d"
        Title="EmojiconPopup" 
        KeyUp="Window_KeyUp"
        Loaded="Window_Loaded" 
        ResizeMode="NoResize" 
        SizeToContent="WidthAndHeight" 
        ShowInTaskbar="False" 
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        Deactivated="Window_Deactivated" Activated="Window_Activated"
        >
    <StackPanel x:Name="ButtonStack" Orientation="Vertical">
        <TextBox x:Name="Search" Padding="2" TextChanged="Search_TextChanged" MinWidth="200"></TextBox>
    </StackPanel>
</Window>
